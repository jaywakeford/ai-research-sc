{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/pdfs/(.*)",
      "dest": "/public/pdfs/$1"
    },
    {
      "src": "/videos/(.*)",
      "dest": "/public/videos/$1"
    },
    {
      "src": "/audio/(.*)",
      "dest": "/public/audio/$1"
    }
  ]
} 