{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "maxDuration": 60,
        "memory": 3008
      }
    }
  ],
  "headers": [
    {
      "source": "/(.*).(mp4|mp3|pdf)",
      "headers": [
        {
          "key": "Accept-Ranges",
          "value": "bytes"
        },
        {
          "key": "Content-Type",
          "value": "application/octet-stream"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, HEAD, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Range"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/pdfs/(.*)",
      "dest": "/public/pdfs/$1"
    },
    {
      "src": "/audio/(.*)",
      "dest": "/public/audio/$1"
    },
    {
      "src": "/videos/(.*)",
      "dest": "/public/videos/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "NEXT_TELEMETRY_DISABLED": "1",
    "NODE_TLS_REJECT_UNAUTHORIZED": "0"
  },
  "regions": ["iad1"],
  "github": {
    "silent": true,
    "autoAlias": true
  }
} 